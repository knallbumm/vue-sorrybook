{
  "name": "vue-sorrybook",
  "version": "0.0.0-alpha.0",
  "description": "The definitions of the components",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/vue-sorrybook/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style": {
      "import": "./dist/style.css"
    },
    "./plugin": {
      "import": "./dist/plugin/index.js",
      "require": "./dist/plugin/index.umd.cjs",
      "types": "./dist/types/vue-sorrybook-plugin/src/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "plugin": [
        "dist/types/vue-sorrybook-plugin/src/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "npm run build-sorrybook && npm run build-sorrybook-plugin && vue-tsc --emitDeclarationOnly",
    "build-sorrybook": "vite build --config packages/vue-sorrybook/vite.config.ts",
    "build-sorrybook-plugin": "vite build --config packages/vue-sorrybook-plugin/vite.config.ts",
    "lint": "npm run lint:files src --",
    "lint:fix": "npm run lint -- --fix",
    "lint:files": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0",
    "typecheck": "vue-tsc --noEmit -p .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knallbumm/vue-sorrybook.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knallbumm/vue-sorrybook/issues"
  },
  "homepage": "https://github.com/knallbumm/vue-sorrybook#readme",
  "dependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@vueuse/core": "^9.5.0",
    "cac": "^6.7.14",
    "events": "^3.3.0",
    "glob": "^8.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^3.3.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "install": "^0.13.0",
    "lint-staged": "^13.0.3",
    "npm": "^8.18.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-inspect": "^0.7.8",
    "vue-tsc": "^0.38.5"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
