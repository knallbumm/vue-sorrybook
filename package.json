{
  "name": "vue-sorrybook",
  "version": "0.0.0-alpha.0",
  "description": "The definitions of the components",
  "type": "module",
  "bin": {
    "sorrybook": "bin/sorrybook.js"
  },
  "files": [
    "dist",
    "bin",
    "sorrybook",
    "assets"
  ],
  "main": "./dist/sorrybook.umd.cjs",
  "module": "./dist/sorrybook.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sorrybook.js",
      "require": "./dist/sorrybook.umd.cjs"
    },
    "./style": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "build": "rimraf dist && vite build && vue-tsc --emitDeclarationOnly",
    "lint": "npm run lint:files src --",
    "lint:fix": "npm run lint -- --fix",
    "lint:files": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0",
    "typecheck": "vue-tsc --noEmit -p .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knallbumm/vue-sorrybook.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/knallbumm"
  },
  "bugs": {
    "url": "https://github.com/knallbumm/vue-sorrybook/issues"
  },
  "homepage": "https://github.com/knallbumm/vue-sorrybook#readme",
  "dependencies": {
    "@rollup/plugin-typescript": "^8.4.0",
    "@vueuse/core": "^8.7.5",
    "cac": "^6.7.14",
    "events": "^3.3.0",
    "glob": "^8.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "install": "^0.13.0",
    "lint-staged": "^13.0.3",
    "npm": "^8.18.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vue-tsc": "^0.38.5"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
